<% group = true if !!@membership and universal_permission_check "can_edit_group_member_roles" -%>
<% @members.each do |u| %>
  <%= render partial: "shared/user_card", locals: {user: u.user, role: u.role, group: group, un: display_name(u.user)} %>
<% end %>
<% if @members.blank? %>
  <p class="text-white">Oops! No more users!</p>
<% end %>